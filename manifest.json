{
    "name": "Tabby Sort",
    "description": "A chrome extension for automatic tab sorting and groupinga",
    "version": "0.1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "cookies",
        "tabs",
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
       "128": "icons/icon128.png"
    }
}